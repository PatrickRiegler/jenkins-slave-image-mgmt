{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {},
  "items": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-slave-base-rhel7",
        "labels": {
          "sdbi": "jenkins-slave-image-mgmt"
        }
      },
      "spec": {
        "tags": [
          {
            "name": "latest",
            "from": {
              "kind": "DockerImage",
              "name": "artifactory.six-group.net/openshift3/jenkins-slave-base-rhel7:latest"
            },
            "importPolicy": {
              "scheduled": true
            }
          }
        ]
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-slave-image-mgmt-pipeline",
        "labels": {
          "sdbi": "jenkins-slave-image-mgmt"
        }
      },
      "spec": {
        "source": {
          "type": "Git",
          "git": {
            "uri": "ssh://git@stash.six-group.net:22/sdbi/jenkins-slave-image-mgmt.git",
            "ref": "develop"
          },
          "sourceSecret": {
            "name": "bitbucket-secret"
          }
        },
        "strategy": {
          "type": "JenkinsPipeline",
          "jenkinsPipelineStrategy": {
            "jenkinsfilePath": "Jenkinsfile"
          }
        },
        "triggers": [
          {
            "type": "ImageChange",
            "imageChange": {
              "from": {
                "kind": "ImageStreamTag",
                "name": "jenkins-slave-image-mgmt:latest"
              }
            }
          }
        ]
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-slave-image-mgmt-build",
        "labels": {
          "sdbi": "jenkins-slave-image-mgmt"
        }
      },
      "spec": {
        "runPolicy": "Serial",
        "source": {
          "type": "Git",
          "git": {
            "uri": "ssh://git@stash.six-group.net:22/sdbi/jenkins-slave-image-mgmt.git",
            "ref": "develop"
          },
          "sourceSecret": {
            "name": "bitbucket-secret"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "jenkins-slave-image-mgmt:latest"
            },
            "forcePull": true
          }
        },
        "output": {
          "to": {
            "kind": "DockerImage",
            "name": "artifactory.six-group.net/sdbi/jenkins-slave-image-mgmt:latest"
          }
        }
      }
    }
  ]
}